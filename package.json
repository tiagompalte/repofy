  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.1",
    "@types/assert": "^1.5.2",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
